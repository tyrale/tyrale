html
  
  link(href="style.css" rel="stylesheet")
  link(href="https://fonts.googleapis.com/css?family=Halant:400,600" rel="stylesheet")
  
  body
    header
      h1 Tyrale is a creative leader focused on User Experience.
    
    .container
      .content-container
      
        mixin content(title, link1, link2, link3)
          h2= title 
          if block
            block
          else
            p No content provided
          a(href=link1) Web
          a(href=link2) Case Study
          a(href=link3) Press
      
        mixin right(title, src, link1, link2, link3)
          .scroll-content-box(class!=attributes.content factor="0")
            video(src=src autoplay loop)
            .scroll-parallax-box.box-1(factor='1600')
              +content(title, link1, link2, link3)
                if block
                  block
                else
                  p No content provided
        mixin left(title, src, link1, link2, link3)
          .scroll-content-box(class!=attributes.content factor="0")
            video(src=src autoplay loop)
            .scroll-parallax-box.box-2(factor='1600')  
              +content(title, link1, link2, link3)
                if block
                  block
                else
                  p No content provided
        
        
        // +right("Avenel", "./img/theopenshift.mp4", "theopenshift.com", "tyrale.com", "./img/theopenshift.jpg")(content="content-box-1")
        //   p A service industry chatbot to help fill open shifts. Simple, clever, and clear.
